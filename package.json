    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.2.1",
    "roslib": "~0.18.0",
    "three": "^0.79.0",
    "tracekit": "^0.4.3",
    "webpack-fail-plugin": "^1.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
