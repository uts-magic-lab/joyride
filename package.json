  "main": "node_modules/.bin/webpack-dev-server",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "NODE_ENV=production webpack --progress",
    "build-dev": "webpack --progress",
    "clean": "rm -rf public",
